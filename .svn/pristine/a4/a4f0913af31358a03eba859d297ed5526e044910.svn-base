page {
  width: 100%;
  /* height: 100%; */
}
scroll-view{
  height: 100vh;
}

.detail {
  width: 100%;
  /* height: 100%; */
}

.scroll-view {
  width: 100%;
}

.tab-wrap {
  height: 70rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  align-items: flex-end;
  justify-content: center;
  width: 100%;
}

.tab-item {
  font-size: 30rpx;
  padding: 0 15rpx;
  position: relative;
}

.tab-item.active {
  font-weight: 900;
}

.tab-item.active:after {
  content: ' ';
  display: block;
  position: absolute;
  bottom: -10rpx;
  left: 0;
  right: 0;
  border-bottom: 2px solid #e70002;
  transform: scaleX(.4);
}

.content-wrap {
  box-sizing: border-box;
  margin-top: 10rpx;
  padding-bottom: 108rpx;
}

.desc-wrap {
  padding: 24rpx;
}

.sale-record,
.sale-record .item,
.sale-record .col {
  box-sizing: border-box;
}

.sale-record {
  padding: 24rpx;
  background-color: #fff;
}

.sale-record .item {
  background-color: #f9f9f9;
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
  height: 148rpx;
  border-radius: 148rpx;
  margin-bottom: 23rpx;
}

.sale-record .item .col {
  text-align: center;
}

.sale-record .item .col-1 {
  width: 148rpx;
  height: 100rpx;
}

.sale-record .item .col-1 .avatar {
  width: 100rpx;
  height: 100rpx;
  display: inline-block;
  border-radius: 100rpx;
  background-image: url('data:image/png;base64,');
  background-size: 100% 100%;
}

.sale-record .item .col-2 {
  width: calc(100% - 296rpx);
  text-align: left;
}

.sale-record .item .col-2 .t {
  font-size: 28rpx;
  line-height: 1;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

.sale-record .item .col-2 .d {
  font-size: 24rpx;
  color: #999999;
  line-height: 1;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  width: 100%;
  margin-top: 16rpx;
}

.sale-record .item .col-3 {
  width: 148rpx;
}

.sale-record .item .col-3 .f {
  font-size: 20rpx;
  color: #999;
}

.sale-record .item .col-3 .s {
  font-size: 24rpx;
  color: #666;
}

.sale-record .avatar-image {
  width: 100%;
  height: 100%;
  overflow: hidden;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  border-radius: 50%;
}

.price-wrap,
.price-wrap .table,
.price-wrap .table .row {
  box-sizing: border-box;
}

.price-wrap {
  padding: 24rpx;
  text-align: center;
}

.price-wrap .title {
  font-size: 28rpx;
  color: #666;
}

.price-wrap .table {
  font-size: 28rpx;
  margin-top: 20rpx;
  border-top: 1px solid #ddd;
  border-right: 1px solid #ddd;
}

.price-wrap .table .row {
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
  height: 56rpx;
}

.price-wrap .table .row .col {
  flex: 1;
  border-bottom: 1px solid #ddd;
  border-left: 1px solid #ddd;
  height: 56rpx;
  line-height: 56rpx;
}

.price-wrap .table .row2 .col {
  color: #999;
}

.visitor-wrap,
.visitor-wrap .item,
.visitor-wrap .item-content,
.visitor-wrap .avatar,
.visitor-wrap .name {
  box-sizing: border-box;
}

.visitor-wrap {
  width: 100%;
}

.visitor-wrap .row {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  align-items: stretch;
  justify-content: space-around;
  padding: 24rpx;
  text-align: center;
}

.visitor-wrap .item {
  box-sizing: border-box;
  width: 20%;
  height: 167rpx;
  padding: 0 10rpx;
}

.visitor-wrap .item-empty {
  height: 0;
}

.visitor-wrap .item-content {
  text-align: center;
  width: 100%;
}

.visitor-wrap .avatar {
  display: inline-block;
  width: 100rpx;
  height: 100rpx;
  background-color: #eee;
  border-radius: 100rpx;
  padding: 0;
  margin: 0;
  line-height: 1;
}

.visitor-wrap .name {
  font-size: 20rpx;
  color: #666;
  padding: 0;
  margin: 0;
  line-height: 1;
}

.visitor-wrap .avatar .image {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}

.action-bar,
.action-bar .item,
.action-bar .icon,
.action-bar .text {
  box-sizing: border-box;
}

.action-bar {
  width: 100%;
  min-height: 108rpx;
  height: calc(108rpx + constant(safe-area-inset-bottom));
  height: calc(108rpx + env(safe-area-inset-bottom));
  padding: 0 65rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-around;
  align-items: center;
  position: fixed;
  left: 0;
  bottom: 0;
  color: #999999;
  font-size: 20rpx;
  background-color: #ffffff;
  text-align: center;
}

.action-bar:after {
  content: ' ';
  display: block;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  width: 100%;
  height: 1px;
  transform: scaleY(0.5);
  -webkit-transform: scaleY(0.5);
  background: #eee;
}

.action-bar .item {
  flex: 1;
  position: relative;
}

.action-bar .item .share {
  position: absolute;
  left: 0;
  right: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
}

.action-bar .item .icon {
  display: inline-block;
  width: 50rpx;
  height: 50rpx;
  background-size: 100% 100%;
}

.action-bar .item .icon.icon1 {
  background-image: url('data:image/png;base64,');
}

.action-bar .item .icon.icon2 {
  background-image: url('data:image/png;base64,');
}

.action-bar .item .icon.icon3 {
  background-image: url('data:image/png;base64,');
}

.no-more {
  text-align: center;
  color: #aaa;
  font-size: 24rpx;
  box-sizing: border-box;
  visibility: hidden;
}

.no-more-show {
  visibility: visible;
}

