@import '/assets/wxss/search.wxss';
@import '/assets/wxss/tutorial.wxss';

page {
    background: #F9F9F9
}

.search_nav {
    height: 164rpx;
}

.headImg_call image.user_head {
    width: 100rpx;
    height: 100rpx;
    margin-right: 25rpx;
    border-radius: 10px;
    background: #fff;
}

.search_nav .head {
    display: flex;
    justify-content: flex-start;
}

.head {
    padding: 8rpx 24rpx 0 24rpx;
    color: #fff;
}

.goPartner {
    width: 175rpx;
    height: 44rpx;
    line-height: 40rpx;
    background-color: #ffffff;
    border-radius: 19px 0px 0px 19px;
    opacity: 0.84;
    position: absolute;
    right: 0;
    top: 26rpx;
    font-size: 30rpx;
    color: #e80608;
    display: flex;
    justify-content: center;
    align-items: center;
}

.price {
    font-size: 24rpx;
    color: #c01f1f;
    line-height: 1;
    bottom: 22rpx;
    position: absolute;
}
.price small{
    color: #999;
}

.commodity_list {
    height: calc(100vh - 328rpx);
    margin-top: 0rpx
}

.commodity_list .commodity_item .title {
    line-height: 32rpx;
}

.title_swiper {
    display: flex;
    align-items: flex-start;
    flex-direction: column;
    justify-content: center;
}

.title_swiper .title {
    font-size: 40rpx;
}

.headImg_call {
    position: relative;
}

.headImg_call .contactIcon {
    width: 44rpx;
    height: 44rpx;
    position: absolute;
    right: 12rpx;
    bottom: 5rpx;
}

.no-data {
    color: #999;
    font-size: 24rpx;
    text-align: center;
    padding: 32rpx 0;

}
.commodity_list .commodity_item .coupon
{
  position: absolute;
}
.commodity_list .commodity_item text.coupon3
{
  color: #fff;
  background: #e70002;  
  display: block;
  font-size: 20rpx; 
  width: 120rpx;
  height: 30rpx;
  line-height: 30rpx;
  text-align: center;
}
.commodity_list .commodity_item  em.text-u
{
  text-decoration: line-through;
  font-size: 20rpx;
}
.modal-mask {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #000;
    opacity: 0.5;
    overflow: hidden;
    z-index: 9000;
    color: #fff;
  }
  .modal-dialog {
    width: 638rpx;
    position: fixed;
    top: 6%;
    left: 6%;
    z-index: 9999;
  }
  .modal-dialog image
  {
    width:638rpx;
    height: 826rpx;
  }
  .modal-title {
    color: #030303;
    text-align: center;
    padding: 15rpx 0;
  }
  .modal-content {
    position: absolute;
    width: 638rpx;
    text-align: center;
    top: 55%;
    color: #fff;
    font-weight: bold;
    height: 160rpx;
  }
  .modal-content .nav{
      width: 60%;
      margin: 0 auto;
  }
  .modal-content .head
  {
      font-size: 50rpx;
      color: #AA0000;
  }
  .modal-content .title
  {
      text-align: left;
      color:#555;
      font-size: 25rpx;
      font-weight: normal;
      font-family: 'Courier New', Courier, monospace;
  }
  
  .modal-dialog .close
  {
    width: 638rpx;
    text-align: right;
    position: fixed;
    top: 2%;
    left: 6%;
  }
  .modal-dialog .close image{
      width: 50rpx;
      height: 50rpx;
  }

  .banner{
    margin-top: 168rpx;
  }
  .banner swiper
  {
    height: 240rpx;
  }
  .banner image
  {
    width: 100%;
    height: 240rpx;
  }