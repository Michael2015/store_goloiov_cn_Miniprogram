page {
  width: 100%;
  height: 100%;
}
view{
  box-sizing: border-box;
}

.detail {
  width: 100%;
  height: 100%;
}

.scroll-view {
  width: 100%;
}

.tab-wrap {
  height: 70rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  align-items: flex-end;
  justify-content: center;
  width: 100%;
}

.tab-item {
  font-size: 30rpx;
  padding: 0 15rpx;
  position: relative;
}

.tab-item.active {
  font-weight: 900;
}

.tab-item.active:after {
  content: ' ';
  display: block;
  position: absolute;
  bottom: -10rpx;
  left: 0;
  right: 0;
  border-bottom: 2px solid #e70002;
  transform: scaleX(.4);
}

.content-wrap {
  box-sizing: border-box;
  margin-top: 10rpx;
  padding-bottom: 108rpx;
}

.desc-wrap {
  padding: 24rpx;
}
.sale-record,
.sale-record .item,
.sale-record .col {
  box-sizing: border-box;
}

.sale-record {
  padding: 24rpx;
  background-color: #fff;
}

.sale-record .item {
  background-color: #f9f9f9;
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
  height: 148rpx;
  border-radius: 148rpx;
  margin-bottom: 23rpx;
}

.sale-record .item .col {
  text-align: center;
}

.sale-record .item .col-1 {
  width: 148rpx;
  height: 100rpx;
}

.sale-record .item .col-1 .avatar {
  width: 100rpx;
  height: 100rpx;
  display: inline-block;
  border-radius: 100rpx;
  background-image: url('data:image/png;base64,');
  background-size: 100% 100%;
}

.sale-record .item .col-2 {
  width: calc(100% - 296rpx);
  text-align: left;
}

.sale-record .item .col-2 .t {
  font-size: 28rpx;
  line-height: 1;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

.sale-record .item .col-2 .d {
  font-size: 24rpx;
  color: #999999;
  line-height: 1;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  width: 100%;
  margin-top: 16rpx;
}

.sale-record .item .col-3 {
  width: 148rpx;
}

.sale-record .item .col-3 .f {
  font-size: 20rpx;
  color: #999;
}

.sale-record .item .col-3 .s {
  font-size: 24rpx;
  color: #666;
}

.sale-record .avatar-image {
  width: 100%;
  height: 100%;
  overflow: hidden;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  border-radius: 50%;
}

.action-bar,
.action-bar .col-1,
.action-bar .shop,
.action-bar .icon,
.action-bar .text,
.action-bar .contact,
.action-bar .buy {
  box-sizing: border-box;
}

.action-bar {
  width: 100%;
  min-height: 108rpx;
  height: calc(108rpx + constant(safe-area-inset-bottom));
  height: calc(108rpx + env(safe-area-inset-bottom));
  position: fixed;
  left: 0;
  bottom: 0;
  background-color: #fff;
  color: #333333;
  font-size: 20rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
  text-align: center;
}
.action-bar:after{
  content: ' ';
  display: block;
  position:absolute;
  left: 0;
  right: 0;
  top: 0;
  width: 100%;
  height: 1px;
  transform:scaleY(0.5);
  -webkit-transform:scaleY(0.5);
  background:#eee;
}

.action-bar .col-1 {
  width: 270rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
  background-color: #fff;
  height: 100%;
  padding: 0 24rpx;
}

.action-bar .buy {
  width: calc(100% - 270rpx);
}

.action-bar .shop,
.action-bar .contact {
  width: 50%;
}
.action-bar .shop .icon {
  width: 50rpx;
  height: 50rpx;
  margin: 0 auto;
}
.action-bar .contact .icon{
  width: 54rpx;
  height: 54rpx;
  margin: 0 auto;
  border-radius: 54rpx;
}

.action-bar .contact .icon {
  width: 54rpx;
  height: 54rpx;
  margin: 0 auto;
  border-radius: 54rpx;
}


.action-bar .shop .image, .action-bar .contact .image {
  width: 100%;
  height: 100%;
}
.action-bar .contact .image{
  border-radius: 50%;
}
.action-bar .buy {
  background: -webkit-linear-gradient(left, #ef5455, #e70103);
  height: 100%;
  font-size: 32rpx;
  color: #fff;
  line-height: 108rpx;
}

.no-more {
  text-align: center;
  color: #aaa;
  font-size: 24rpx;
  box-sizing: border-box;
  visibility: hidden;
}

.no-more-show {
  visibility: visible;
}

.zhuan{
  width: 76rpx;
  height: 76rpx;
  display: flex;
  flex-wrap: nowrap;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  position: fixed;
  bottom: 240rpx;
  right: 0;
  color: #fff;
  background-color: #e91113;
  border-radius: 50%;
  font-size: 28rpx;
}