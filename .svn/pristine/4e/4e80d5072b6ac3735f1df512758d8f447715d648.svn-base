.title-wrap, .title, .p, .s, .popup-platoon, .c, .t, .rule, .d, .item, .btn, .btn-wrap, .close{
  box-sizing: border-box;
}
.title-wrap{
  position: relative;
  padding: 0 24rpx;
  height: 260rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  align-items: center;

}
