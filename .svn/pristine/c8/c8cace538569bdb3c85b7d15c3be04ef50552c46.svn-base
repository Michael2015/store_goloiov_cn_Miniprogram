page {
    background: #F0F0F0;
    box-shadow: inset 0 15rpx 10rpx -15rpx #eeeeee;
    height: 100vh;
    padding-top: 24rpx;
}

.item {
    height: 108rpx;
    background: #fff;
    font-size: 28rpx;
    border-bottom: 1rpx solid #F0F0F0;
    padding: 0 50rpx 0 32rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.item image {
    width: 14rpx;
    height: 25rpx;
}

.disclaimer {
    width: 100vw;
    height: auto;
    position: fixed;
    top: 85vh;
    text-align: center;
    font-size: 24rpx;
    color:#a7a7a7;
    line-height: 1.8;
}

.disclaimer view:nth-child(1) {
    /* margin-bottom: 24rpx; */
    color: #3c6ad2;
}
