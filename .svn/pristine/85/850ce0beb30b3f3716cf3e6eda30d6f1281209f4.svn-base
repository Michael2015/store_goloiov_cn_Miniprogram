page{
  background-color: #f9f9f9;
  padding-bottom: 124rpx;
}
view{
  box-sizing: border-box;
  padding: 0;
  margin: 0;
}
.detail{
  font-size: 28rpx;
  padding: 0;
  color: #333333;
}
.detail .status{
  font-size: 36rpx;
  font-weight: 700;
  line-height: 1.1;
  padding: 0 24rpx;
}
.detail .info-wrap{
  position: relative;
}
.detail .user-info{
  padding: 0 24rpx;
  position: relative;
  z-index: 3;
}
.detail .user-info > view{
  height: 176rpx;
  background-color: #fff;
  width: 100%;
  border-radius: 4px;
  margin-top: 26rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: center;
  align-items: center;
  box-shadow:2px 2px 3px #eee;
}
.detail .user-info .col-1{
  width: 70rpx;
}
.detail .user-info .col-1 .icon{
  width: 28rpx;
  height: 40rpx;
  background-image: url('data:image/png;base64,');
  background-size: 100% 100%;
  margin: 0 auto;
}
.detail .user-info .col-2{
  width: calc(100% - 70rpx);
  padding-left: 36rpx;
  padding-right: 24rpx;
}
.detail .user-info .col-2 .f .name{
  margin-left: 10rpx;
}
.detail .user-info .col-2 .f .tel{
  margin-left: 15rpx;
}
.detail .user-info .col-2 .s{
  font-size: 24rpx;
  color: #999;
  margin-top: 10rpx;
}
.detail .goods-info-wrap{
  width: 100%;
  position: relative;
  background-color: #fff;
  z-index: 1;
  margin-top: -88rpx;
}
.detail .goods-info{
  padding: 88rpx 50rpx 0;
}
.detail .goods{
  width: 100%;
  height: 180rpx;
  display: flex;
  flex-wrap: nowrap;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  position: relative;
}
.detail .goods:after{
  content: ' ';
  display: block;
  position:absolute;
  left: 0;
  right: 0;
  bottom: 0;
  width: 100%;
  height: 1px;
  transform:scaleY(0.5);
  -webkit-transform:scaleY(0.5);
  background:#eee;
}
.detail .goods .col-1{
  width: 60rpx;
  height: 60rpx;
}
.detail .goods .col-1 image{
  width: 100%;
  height: 100%;
}
.detail .goods .col-2{
  width: calc(100% - 120rpx);
  height: 60rpx;
  line-height: 60rpx;
}
.detail .goods .col-2 .f{
  width: 100%;
  position: relative;
}
.detail .goods .col-2 .f .title{
  width: calc(100% - 100rpx);
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  font-size: 28rpx;
  font-weight: 800;
}
.detail .goods .col-2 .f .price{
  width: 100rpx;
  position: absolute;
  top: 2px;
  right: 0;
  text-align: right;
  color: #e70002;
  font-size: 24rpx;
}
.detail .goods .col-2 .s{
  font-size: 24rpx;
  position: relative;
  margin-top: 10rpx;
}
.detail .goods .col-2 .s .desc{
  color: #999;
}
.detail .goods .col-2 .s .qty{
  color: #666;
  position: absolute;
  right: 0;
  top: 2px;
}
.detail .goods-info .remark{
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: flex-start;
  padding: 24rpx 0;
  font-size: 24rpx;
  position: relative;
}
.detail .goods-info .remark:after{
  content: ' ';
  display: block;
  position:absolute;
  left: 0;
  right: 0;
  bottom: 0;
  width: 100%;
  height: 1px;
  transform:scaleY(0.5);
  -webkit-transform:scaleY(0.5);
  background:#eee;
}
.detail .goods-info .remark .col-1{
  width: 108rpx;
  color: #999;
}
.detail .goods-info .remark .col-2{
  width: calc(100% - 108rpx);
  color: #666;
  line-height: 1.6;
}

.detail .goods-info .total{
  font-size: 28rpx;
  text-align: right;
  height: 88rpx;
  line-height: 88rpx;
}
.detail .goods-info .total text{
  color: #e70002;
}

.detail .order-info{
  margin-top: 24rpx;
  width: 100%;
  font-size: 28rpx;
  background-color: #fff;
  padding: 0 24rpx 24rpx;
}
.detail .order-info .title{
  font-weight: 800;
  height: 74rpx;
}
.detail .order-info .row{
  display: flex;
  flex-wrap: nowrap;
  flex-direction: row;
  align-items: center;
  height: 67rpx;
  color: #666;
}
.detail .order-info .row .col-1{
  width: 5em;
}
.detail .order-info .row .col-2{
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
  width: calc(100% - 5em);
}
.detail .order-info .row .col-2 > div{
  flex: 1;
}
.detail .order-info .row .col-2 .right{
  font-size: 20rpx;
  padding: 10rpx 20rpx;
  border-radius: 30rpx;
  border: 1px solid #e4e4e4;
  text-align: right;
}

.detail .action-bar{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: flex-end;
  align-items: center;
  background-color: #fff;
}
.detail .action-bar .btn{
  font-size: 24rpx;
  padding: 16rpx 30rpx;
  color: #f35759;
  border: 1px solid #f27c7e;
  border-radius: 30rpx;
  margin-right: 24rpx;
}
.detail .title .contactIcon
{
  float: left;
  height: 35rpx;
  width: 35rpx;
  margin-right: 10rpx;
}
.detail .title .l
{
  margin-right: 20rpx;
  float: left;
}
.detail .area
{
  padding: 24rpx;
}
.detail .area textarea
{
  color:#000;
  background:#eee;
  padding:2%;
  width:96%;
}
.upUpoto
{
  padding: 24rpx;
}
.upUpoto .photo_title {
  height: 88rpx;
  display: flex;
  align-items: center;
  color: #666;
}

.upUpoto .photo_title .min {
  font-size: 20rpx;
  color: #999;
}

.add_img {
  width: 32rpx;
  height: 32rpx;
}

.selectPhoto {
  width: 120rpx;
  height: 120rpx;
  border: solid 1rpx #e4e4e4;
  display: flex;
  align-items: center;
  justify-content: center;
}

.user_select_photo {
  width: 120rpx;
  height: 120rpx;
  border: solid 1rpx #e4e4e4;
  margin-right: 15rpx;
}

.photo_view {
  margin-top:-122rpx;
  width: auto;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  flex-wrap: wrap;
  margin-left:135rpx;
}
.submit
{
min-height:96rpx;
height:calc(96rpx + env(safe-area-inset-bottom));
height:calc(96rpx + env(safe-area-inset-bottom));
width:100vw;
background-color:#e70002;
font-size:36rpx;
color:#fff;
border-radius:0;
position:fixed;
bottom:0;
left:0;

}




































