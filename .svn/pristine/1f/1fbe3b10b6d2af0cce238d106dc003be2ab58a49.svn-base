@import "./utils/wxParse/wxParse.wxss";
button:after {
    border: none;
}
.border-bottom-global{
    position: relative;
}
.border-bottom-global:after{
    content: ' ';
    display: block;
    position: absolute;
    width: 100%;
    height: 1px;
    background: #e4e4e4;
    transform: scaleY(.5);
    left:0;
    bottom:0;
}