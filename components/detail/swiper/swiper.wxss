.swiper-wrap{
  position: relative;
}
.swiper{
  height: 630rpx;
}
.slide-image{
  width: 100%;
  height: 100%;
}
.indicator{
  position: absolute;
  bottom: 40rpx;
  right: 40rpx;
  font-size: 18rpx;
  color: #fff;
  background-color: rgba(0, 0, 0, .5);
  padding: 5rpx 12rpx;
  border-radius: 30rpx;
}