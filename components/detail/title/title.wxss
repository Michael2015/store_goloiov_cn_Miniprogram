.popup-platoon .mask{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 98;
  background-color: rgba(0, 0, 0, .2);
}
.popup-platoon .main{
  background-color: #fff;
  width: 600rpx;
  position: fixed;
  padding-top: 26rpx;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 99;
  -webkit-border-radius: 32rpx;
  -moz-border-radius: 32rpx;
  border-radius: 32rpx;
  text-align: center;
}

.popup-platoon .main .title{
  font-size: 26rpx;
  width: 100%;

}
.popup-platoon .main .title .t{
  color: #646464;
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
  padding: 0 78rpx;
}
.popup-platoon .main .title .t .col-2{
  width: 6em;
}
.popup-platoon .main .title .t .col-1, .popup-platoon .main .title .t .col-3{
  width: calc((100% - 6em) / 2);
  height: 1rpx;
}
.popup-platoon .main .title .t .col-1 {
  background: -webkit-linear-gradient(left, #fff, #ddd);
  background: -o-linear-gradient(left, #fff, #ddd);
  background: -moz-linear-gradient(left, #fff, #ddd);
  background: linear-gradient(left, #fff, #ddd);
}
.popup-platoon .main .title .t .col-3 {
  background: -webkit-linear-gradient(right, #fff, #ddd);
  background: -o-linear-gradient(right, #fff, #ddd);
  background: -moz-linear-gradient(right, #fff, #ddd);
  background: linear-gradient(right, #fff, #ddd);
}
.popup-platoon .main .title .c{
  margin-top: 20rpx;
  padding: 0 32rpx;
  font-size: 30rpx;
  color:#373737;
}
.popup-platoon .main .rule{
  margin-top: 16rpx;
  padding-bottom: 30rpx;
}
.popup-platoon .main .rule .t{
  font-size: 26rpx;
  color:#5297e7;
  margin-bottom: 54rpx;
}
.popup-platoon .main .rule .d{
  margin-top: 12rpx;
}
.popup-platoon .main .rule .item{
  font-size: 26rpx;
  line-height: 1.5;
  padding: 0 48rpx;
  text-align: left;
  margin: 16rpx 0;
  color:#666;
  position: relative;
}
.popup-platoon .main .rule .item:after{
  position: absolute;
  content: ' ';
  display: block;
  width: 10rpx;
  height: 10rpx;
  background: #666;
  border-radius: 50%;
  top: 18rpx;
  left: 24rpx;
}
.popup-platoon .main .btn-wrap{
  height: 115rpx;
  position: relative;
}
.popup-platoon .main .btn-wrap .btn{
  height: 100%;
  line-height: 110rpx;
  font-size: 30rpx;
  color: #ea2224;
}
.popup-platoon .main .btn-wrap:after{
  content: ' ';
  display: block;
  position:absolute;
  left: 0;
  right: 0;
  top: 0;
  width: 100%;
  height: 1px;
  transform:scaleY(0.5);
  -webkit-transform:scaleY(0.5);
  background:#a5a5a5;
}
.popup-platoon .main .close{
  position: absolute;
  top: 24rpx;
  right: 24rpx;
  width: 28rpx;
  height: 28rpx;
  background-image: url('data:image/png;base64,');
  -webkit-background-size: 100% 100%;
  background-size: 100% 100%;
}
