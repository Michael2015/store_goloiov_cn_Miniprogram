/* pages/partner/personal/wallet/cash.wxss */
page{
  background: #f9f9f9;
  min-height: 100vh;
}
.wrap{
  /* padding-top: 24rpx; */
  padding-bottom: 24rpx;
}
.top-tab{
  display: flex;
  display: -webkit-flex;
  text-align: center;
  height: 82rpx;
  line-height: 82rpx;
  align-items: center;
  background: #fff;
  margin-bottom: 22rpx;
  font-size: 28rpx;
}
.top-tab >view{
  flex: 1;
  -webkit-flex:1;
  position: relative;
}
.top-tab >view.active{
  color:#e70002;
}
.top-tab >view.active:after{
  content: ' ';
  display: block;
  width: 160rpx;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  -webkit-transform: translateX(-50%);
  bottom: 0;
  background: #e70002;
  height: 4rpx;
}
.form{
  background: #fff;
  padding:0 24rpx;
}
.form .input-wrap:after{
  /* content: ' ';
  display: block;
  position: absolute;
  height: 1px;
  transform: scaleY(.5);
  left: 0;
  width: 100%;
  bottom: 0;
  background: #e4e4e4; */
}
.form .title{
  font-size:28rpx;
  color:#333;
  line-height: 72rpx;
}
.form .title text{
  font-size:28rpx;
  color:#333;
}
.form .input-wrap{
  display: flex;
  align-items: center;
  min-height: 100rpx;
  position: relative;
}
.form .input-wrap .yuan{
  width: 36rpx;
  font-size: 36rpx;
  color:#333;
}
.form .input-wrap input{
  flex: 1;
  /* background: gold; */
  padding: 0 12rpx;
  font-size: 30rpx;
}
.form .bottom{
  line-height: 88rpx;
  font-size: 28rpx;
}
.form .bottom .all{
  color:#999;
}
.form .bottom .set-all{
  color:#e70002;
  float: right;
  line-height: 88rpx;
}
.btn{
  line-height: 90rpx;
  border-radius: 45rpx;
  text-align: center;
  font-size: 32rpx;
  color:#fff;
  background: linear-gradient(to right, #ef5456, #e70002);
  margin:24rpx;
  margin-bottom: 0;
}
.record{
  line-height: 98rpx;
  text-align: center;
  font-size: 28rpx;
  color: #e70002;
}
.record.navigator-hover{
  opacity: 1;
  background-color: rgba(0, 0, 0, 0);
}
.rule{
  padding: 0 24rpx;
  font-size: 24rpx;
  color:#999;
  line-height: 40rpx;
}
.rule .title{
  margin-bottom: 10rpx;
}
.rule .light{
  color:#666;
}
.bank-card, .bank-card view{
  box-sizing: border-box;
  line-height: 1;
}

.bank-card{
  padding: 30rpx 26rpx;
  margin-bottom: 24rpx;
  font-size: 28rpx;
  background-color: #fff;
}
.bank-card .t{
  color: #999999;
  font-size: 24rpx;
}

.bank-card .t text{
  color: #333;
  font-size: 28rpx;
  padding-left: 10rpx;
}
.bank-card .card{
  width: 100%;
  height: 218rpx;
  background-color: #ff4646;
  background-image: url('data:image/png;base64,');
  -webkit-background-size: 100% 100%;
  background-size: 100% 100%;
  display: flex;
  flex-wrap: nowrap;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  margin-top: 20rpx;
  color: #edf2fa;
  padding: 0 30rpx;
  -webkit-border-radius: 10rpx;
  -moz-border-radius: 10rpx;
  border-radius: 10rpx;
  position: relative;
}
.bank-card .card .bname{
  font-size: 34rpx;
  font-weight: 800;
}
.bank-card .card .uname{
  margin-top: 18rpx;
  font-size: 22rpx;
}

.bank-card .card .num{
  margin-top: 40rpx;
  font-size: 40rpx;
}

.bank-card .card .num text{
  padding-right: 10rpx;
  position: relative;
  top: 2px;
}
.bank-card .card .btn-del{
  position: absolute;
  top: 42rpx;
  right: 30rpx;
  font-size: 26rpx;
  height: 44rpx;
  line-height: 44rpx;
  border: 1px solid #edf2fa;
  -webkit-border-radius: 44rpx;
  -moz-border-radius: 44rpx;
  border-radius: 44rpx;
  padding: 0 30rpx;
}

.bank-card .btn-change{
  text-align: center;
  margin-top: 33rpx;
  color: #e70002;
}

.bank-card .add{
  width: 100%;
  height: 212rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;justify-content: center;
  align-items: center;
}

.bank-card .add .btn{
  width: 104rpx;
  height: 104rpx;
  background-color: #ec3a3c;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.bank-card .add .btn text{
  font-size: 70rpx;
  position: relative;
  top: -6rpx;
}
.bank-card .add-text{
  text-align: center;
  font-size: 30rpx;
  margin-bottom: 5rpx;
}
