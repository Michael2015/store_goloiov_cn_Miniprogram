page {
  width: 100%;
  /* height: 100%; */
}
scroll-view{
  height: 100vh;
}

.detail {
  width: 100%;
  /* height: 100%; */
}

.scroll-view {
  width: 100%;
}

.tab-wrap {
  height: 70rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  align-items: flex-end;
  justify-content: center;
  width: 100%;
}

.tab-item {
  font-size: 30rpx;
  padding: 0 15rpx;
  position: relative;
}

.tab-item.active {
  font-weight: 900;
}

.tab-item.active:after {
  content: ' ';
  display: block;
  position: absolute;
  bottom: -10rpx;
  left: 0;
  right: 0;
  border-bottom: 2px solid #e70002;
  transform: scaleX(.4);
}

.content-wrap {
  box-sizing: border-box;
  margin-top: 10rpx;
  padding-bottom: 108rpx;
}

.desc-wrap {
  padding: 24rpx;
}

.sale-record,
.sale-record .item,
.sale-record .col {
  box-sizing: border-box;
}

.sale-record {
  padding: 24rpx;
  background-color: #fff;
}

.sale-record .item {
  background-color: #f9f9f9;
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
  height: 148rpx;
  border-radius: 148rpx;
  margin-bottom: 23rpx;
}

.sale-record .item .col {
  text-align: center;
}

.sale-record .item .col-1 {
  width: 148rpx;
  height: 100rpx;
}

.sale-record .item .col-1 .avatar {
  width: 100rpx;
  height: 100rpx;
  display: inline-block;
  border-radius: 100rpx;
  background-image: url('data:image/png;base64,');
  background-size: 100% 100%;
}

.sale-record .item .col-2 {
  width: calc(100% - 296rpx);
  text-align: left;
}

.sale-record .item .col-2 .t {
  font-size: 28rpx;
  line-height: 1;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

.sale-record .item .col-2 .d {
  font-size: 24rpx;
  color: #999999;
  line-height: 1;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  width: 100%;
  margin-top: 16rpx;
}

.sale-record .item .col-3 {
  width: 148rpx;
}

.sale-record .item .col-3 .f {
  font-size: 20rpx;
  color: #999;
}

.sale-record .item .col-3 .s {
  font-size: 24rpx;
  color: #666;
}

.sale-record .avatar-image {
  width: 100%;
  height: 100%;
  overflow: hidden;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  border-radius: 50%;
}

.price-wrap,
.price-wrap .table,
.price-wrap .table .row {
  box-sizing: border-box;
}

.price-wrap {
  padding: 24rpx;
  text-align: center;
}

.price-wrap .title {
  font-size: 28rpx;
  color: #666;
}

.price-wrap .table {
  font-size: 28rpx;
  margin-top: 20rpx;
  border-top: 1px solid #ddd;
  border-right: 1px solid #ddd;
}

.price-wrap .table .row {
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
  height: 56rpx;
}

.price-wrap .table .row .col {
  flex: 1;
  border-bottom: 1px solid #ddd;
  border-left: 1px solid #ddd;
  height: 56rpx;
  line-height: 56rpx;
}

.price-wrap .table .row2 .col {
  color: #999;
}

.visitor-wrap,
.visitor-wrap .item,
.visitor-wrap .item-content,
.visitor-wrap .avatar,
.visitor-wrap .name {
  box-sizing: border-box;
}

.visitor-wrap {
  width: 100%;
}

.visitor-wrap .row {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  align-items: stretch;
  justify-content: space-around;
  padding: 24rpx;
  text-align: center;
}

.visitor-wrap .item {
  box-sizing: border-box;
  width: 20%;
  height: 167rpx;
  padding: 0 10rpx;
}

.visitor-wrap .item-empty {
  height: 0;
}

.visitor-wrap .item-content {
  text-align: center;
  width: 100%;
}

.visitor-wrap .avatar {
  display: inline-block;
  width: 100rpx;
  height: 100rpx;
  background-color: #eee;
  border-radius: 100rpx;
  padding: 0;
  margin: 0;
  line-height: 1;
}

.visitor-wrap .name {
  font-size: 20rpx;
  color: #666;
  padding: 0;
  margin: 0;
  line-height: 1;
}

.visitor-wrap .avatar .image {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}

.no-more {
  text-align: center;
  color: #aaa;
  font-size: 24rpx;
  box-sizing: border-box;
  visibility: hidden;
}

.no-more-show {
  visibility: visible;
}

.comment-record .avatar-image
{
  width: 50rpx;
  height: 50rpx;
}
.comment-record .item .col .avatar
{
  width: 50rpx;
  height: 50rpx;
}
.comment-record .item .col,.comment-record .t1,.comment-record .t2,.comment-record .t3
{
  height: 50rpx;
}
.comment-record .item .col
{
  text-align: left;
  width: 100%;
}
.comment-record .t1
{
  float: left;
}
.comment-record .t2
{
  float:left;
 font-size:31rpx;
 padding-left:15rpx;
 line-height: 50rpx;
}
.comment-record .t3
{
  float: right;
  width:50%;
  overflow: hidden;
  line-height: 50rpx;
}
.comment-record .t3 image
{
  height: 35rpx;
  width: 35rpx;
  float: right;
  margin-top: 7.5rpx;
}

.comment-record .col .s
{
  font-size: 25rpx;
  line-height: 35rpx;
  display: flex;
}
.comment-record .item .col-h
{
  height: auto;
  margin-top: 10rpx;
}

.comment-record .item
{
  height: auto;
  display: block;
  margin-bottom: 30rpx;
  background:transparent;
  border-radius:0;
}
.comment-record .item .col .s .avatar-image
{
  height: 120rpx;
  width: 120rpx;
  border-radius: 0;
  margin-right:20rpx;
}
.comment-record .item .col .time{
  font-size: 25rpx;
  color: #aaa;
}
.seckill{
  width: 100%;
  position: relative;
}
.seckill image
{
  width: 100%;
  height: 180rpx;
}
.seckill .count_back{
  color: #ffffff;
  position: absolute;
  left:30%;
  top: 40%;
  font-size: 80rpx;
  font-weight: bold;
}

.action-bar{
  width: 100%;
  min-height: 108rpx;
  position: fixed;
  left: 0;
  bottom: 0;
  background-color: #fff;
  color: #333333;
  font-size: 20rpx;
  display: flex;
  text-align: center;
}
.bar-item{
  padding: 25rpx 20rpx;
  border:1px solid rgba(224,224,224,1);
  display: flex;
  box-sizing: border-box;
}

.bar-item .image{
  width: 50rpx;
  height: 50rpx;
  margin: auto;
}
.bar-item .image-avatar{
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  overflow: hidden;
  margin: auto;
}

.bar-item .text{
  font-size: 24rpx;
  color: #666666;
  margin-left: 15rpx;
  line-height: 58rpx;
}
.bar-item .txt-top2
{
    line-height: 58rpx;
}
.bar-item .txt{
  margin-left: 15rpx;
  font-size: 24rpx;
  color: #666666;
}
.txt .txt-top{
  text-align: start;
}
.txt .txt-botoom{
  font-size: 20rpx;
  color: #999999;
}

.bar-button{
  flex: 1;
  background: #E60000;
  font-size: 32rpx;
  color: #fff;
  line-height: 108rpx;
}

.fenxiang-mask{
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  background:rgba(0, 0, 0, 0.5);
  overflow: hidden;
  z-index: 9999;
  color: #fff;
}
.select{
  width: 100%;
  height: 200rpx;
  position: fixed;
  left: 0;
  bottom: 50%;
  z-index: 10000;
  display: flex;
  flex-direction: column;
  background: #fff;
}
button{
  background-color: #fff;
}
.select-top{
  display: flex;
  justify-content: space-around;
}
.top-item{
  width: 220rpx;
}
.select-top .item-botton{
  height: 160rpx;
  position: relative;
  margin: 20rpx;
  padding: 0;
  line-height: 2.5em;
}
.fenXiang-img{
  width: 120rpx;
}
.botton-title{
  font-size: 25rpx;
  position: absolute;
  left: 50%;
  width: 100%;
  top: 60%;
  color: #555;
  transform: translateX(-50%)
}
.select-bottom{
  margin-top: 10rpx;
  height: 40rpx;
}
.select-bottom .exit{
  height: 40rpx;
  width: 40rpx;
  border-radius: 50rpx;
  font-size: 35rpx;
  position: absolute;
  top: -40rpx;
  right: 0;
  padding: 0;
  line-height: 35rpx;
}